{
  "name": "yet-another-cookbook",
  "version": "1.0.0",
  "dependencies": {
    "babel-polyfill": "6.16.0",
    "bootstrap": "3.3.7",
    "jquery": "3.1.1",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "4.4.5",
    "react-router": "3.0.0",
    "redux": "3.6.0",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-core": "6.18.2",
    "babel-jest": "16.0.0",
    "babel-loader": "6.2.7",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "enzyme": "2.5.1",
    "jest": "16.0.2",
    "react-addons-test-utils": "15.3.2",
    "rimraf": "2.5.4",
    "source-map-loader": "0.1.5",
    "ts-loader": "2.3.2",
    "ts-jest": "19.0.6",
    "tslint": "3.15.1",
    "tslint-microsoft-contrib": "2.0.13",
    "typescript": "2.4.2",
    "typings": "1.5",
    "webpack": "2.1.0-beta.21"
  },
  "scripts": {
    "postinstall": "typings install",
    "build": "npm run webpack -- --optimize-minimize",
    "develop": "npm run webpack -- --color true --debug --output-pathinfo --devtool inline-source-map --watch",
    "pretest": "rimraf ./build ",
    "test": "tsc --outDir ./build && jest",
    "lint": "tslint -c ./tslint.json ./src/**/*.ts ./src/**/*.tsx",
    "prewebpack": "rimraf ./dist",
    "webpack": "webpack"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/ts-jest/preprocessor.js",
    "testRegex": "(/tests/.*|\\.test)\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "globals": {
      "__TS_CONFIG__": {
        "target": "es5",
        "module": "commonjs",
        "jsx": "react"
      }
    }
  }
}
