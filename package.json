{
  "name": "yet-another-cookbook",
  "version": "1.0.0",
  "dependencies": {
    "babel-polyfill": "6.13.0",
    "bootstrap": "3.3.7",
    "jquery": "3.1.0",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "react-redux": "4.4.5",
    "react-router": "2.7.0",
    "redux": "3.5.2",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-core": "6.13.2",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "react-addons-test-utils": "15.3.1",
